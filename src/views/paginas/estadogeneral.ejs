        <div class="table-responsive">
            <table class="table table-striped table-bordered table-sm table-hover " style="width: 100%">
                <thead>
                    <tr>
                        <th>
                            <h4>Nombre</h4>
                        </th>
                        <th>
                            <h4>N° de carpeta</h4>
                        </th>
                        <th>
                            <h4>Ubicación</h4>
                        </th>
                        <th>
                            <h4>Comitente</h4>
                        </th>
                        <th>
                            <h4>Estado</h4>
                        </th>
                        <th>
                            <h4>Fecha firma de contrato</h4>
                        </th>
                </thead>
                <tbody>
                    </tr>
                    <% results.forEach((clientes)=>{ %>
                         
                        <tr>
                            <th>
                                <%= clientes.Nombre %>
                            </th>
                            <th>
                                <%=clientes.NCarpeta%>
                            </th>
                            <th>
                                <%=clientes.Ubicación%>
                            </th>
                            <th>
                                <%=clientes.Comitente%>
                            </th>
                            <th>
                                <%=clientes.Estado%>
                            </th>
                            <th>
                                <%= moment(clientes.Fechafirmacontrato).format('DD-MM-YYYY')%>
                            </th>
                            <th><a href="/edit/<%=clientes.id%>" class="btn btn-outline-info">editar</a>
                            </tr></th>
                        <% })%>
                </tbody>
            </table>
