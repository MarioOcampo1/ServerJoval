                                            tareasporcarpeta.PCaprobado=="EnGestion" ){%>
                                            <p>Se encuentra en gestión </p>
                                            <%}%>
                                </th>
                                <th class="table-primary">
                                    <!-- Configuracion cuadro "Segunda Parte" -->
                                    <%if(tareasporcarpeta.AvisoInicioObras=="ok" && tareasporcarpeta.CertificadoRT=="ok"
                                        && tareasporcarpeta.Interferencias=="ok" && tareasporcarpeta.Permisos=="ok" &&
                                        tareasporcarpeta.Programadeseguridad=="ok" ){%>
                                        Finalizada
                                        <%}%>
                                            <%if(tareasporcarpeta.MailAutorizacion=="EnGestion" ||
                                                tareasporcarpeta.CertificadoRT=="EnGestion" ||
                                                tareasporcarpeta.Interferencias=="EnGestion" ||
                                                tareasporcarpeta.Permisos=="EnGestion" ||
                                                tareasporcarpeta.Programadeseguridad=="EnGestion" ){%>
                                                En Gestion
                                                <%}%>
                                </th>
                                <th class="table-primary">

                                </th>
                                <th class="table-primary">
                                    <!-- Configuracion cuadro "Caos" -->
                                    <%if(tareasporcarpeta.DocInspeccion=="ok"&& tareasporcarpeta.PlanosyCroquis=="ok" &&
                                        tareasporcarpeta.ActasFinales=="ok" && tareasporcarpeta.InformesFinales=="ok" &&
                                        tareasporcarpeta.ConformedePermisos=="ok"
                                        &&tareasporcarpeta.PresentacionFinal=="ok" ){%>
                                        Finalizada
                                        <%}%>
                                            <%if(tareasporcarpeta.DocInspeccion=="EnGestion"||
                                                tareasporcarpeta.PlanosyCroquis=="EnGestion"
                                                ||tareasporcarpeta.ActasFinales=="EnGestion" ||
                                                tareasporcarpeta.InformesFinales=="EnGestion"
                                                ||tareasporcarpeta.ConformedePermisos=="EnGestion"
                                                ||tareasporcarpeta.PresentacionFinal=="EnGestion" ){%>
                                                En Gestion
                                                <%}%>
                                </th>
                                <th class="table-primary">
                                    <%if(tareasporcarpeta.PresentacionFinal=="ok"){%>
                                        Finalizada
                                        <%}%>
                                </th>

                            </tr>
                        </tbody>




                        <% } %>
                    </table>
