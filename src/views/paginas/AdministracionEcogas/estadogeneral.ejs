<!DOCTYPE html>
<html lang="en">

<head>
    <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge"> -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Joval S.A.</title>
    <link rel="stylesheet" type="text/css"
        href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/dataTables.bootstrap5.min.css">
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <title>Estado General</title>
</head>

<body>
    <%- include('./partials/menusuperior.ejs')%>
        <h1 class="text-center">Estado General</h1>

        <div class="container-fluid">
            <div class="table-responsive-sm ">
                <table class="table table-striped table-bordered border-secondary  text-center table-sm "
                    style="width: 100%">
                    <thead>
                        <tr>
                            <th class="text-center text-light bg-dark  " colspan="6">Datos de carpeta</th>
                            <th class="text-center text-light bg-dark " colspan="5">Preliminar</th>
                            <th class="text-center text-light bg-dark " colspan="4">1era parte</th>
                            <th class="text-center text-light bg-primary " colspan="5">2da parte</th>
                            <th class="text-center text-light bg-dark " colspan="8">Caos</th>
                        </tr>
                        <tr>
                            <th>
                                <h4>Nombre</h4>
                            </th>
                            <th>
                                <h4>N° de carpeta</h4>
                            </th>
                            <th>
                                <h4>Ubicación</h4>
                            </th>
                            <th>
                                <h4>Comitente</h4>
                            </th>
                            <th>
                                <h4>Estado</h4>
                            </th>
                            <th>
                                <h4>Fecha firma de contrato</h4>
                            </th>

                            <th class="text-light bg-primary">
                                Documentación del terreno
                            </th>
                            <th class="text-light bg-primary">
                                Documentación de la Sociedad
                            </th>
                            <th class="text-light bg-primary">
                                Documentación contractual
                            </th>
                            <th class="text-light bg-primary">
                                Comercial
                            </th>
                            <th class="text-light bg-primary">
                                Tecnica
                            </th>

                            <th>
                                Permisos especiales
                            </th>
                            <th>
                                Documentación de obra
                            </th>
                            <th>
                                Seguridad
                            </th>
                            <th>
                                Interferencias
                            </th>
                            <th>
                                Permisos
                            </th>
                            <th>
                                Plan de trabajos
                            </th>
                            <th>
                                Matriculas
                            </th>
                            <th>
                                Ambiente
                            </th>
                            <th>
                                Nota cumplimento de normas vigentes
                            </th>
                            <th>
                                DDJJ NAG 153
                            </th>
                            <th>
                                Avisos
                            </th>
                            <th>
                                Documentación de inspección
                            </th>
                            <th>
                                Comunicación de obras
                            </th>
                            <th>
                                Actas finales de Ecogas
                            </th>
                            <th>
                                Plano y croquis
                            </th>
                            <th>
                                Conforme de entidades
                            </th>
                            <th>
                                Prueba de hermeticidad
                            </th>
                            <th>
                                Informe finales
                            </th>
                            <th>
                                Presentación final
                            </th>
                            <th>
                                Habilitación de obra
                            </th>
                           
                        </tr>
                    </thead>
                    <tbody>
                        <%clientes=results%>
                        <% if(clientes){%>
                        <% results.forEach((clientes)=>{ %>
                            <tr>
                                <th>
                                    <a href="/editarTareas/<%=clientes.id%>" class="">
                                        <%=clientes.Nombre%>
                                    </a>
                                </th>
                                <th>
                                    <a href="/editarTareas/<%=clientes.id%>" class="">
                                        <%=clientes.NCarpeta%>
                                    </a>
                                </th>
                                <th>
                                    <%=clientes.Ubicacion%>
                                </th>
                                <th>
                                    <%=clientes.Comitente%>
                                </th>
                                <th>
                                    <%=clientes.Estado%>
                                        <%if(clientes.Estado=="Sin presentar"){%>
                                            <progress max="4" value="0"></progress>
                                            <%}%>
                                                <%if(clientes.Estado=="1ra parte"){%>
                                                    <progress max="4" value="1"></progress>
                                                    <%}%>

                                                        <%if(clientes.Estado=="2da parte"){%>
                                                            <progress max="4" value="2"></progress>
                                                            <%}%>

                                                                <%if(clientes.Estado=="CAOS"){%>
                                                                    <progress max="4" value="3"></progress>
                                                                    <%}%>

                                                                        <%if(clientes.Estado=="Finalizada"){%>
                                                                            <progress max="4" value="4"></progress>
                                                                            <%}%>
                                                                                <%if(clientes.Estado=="en obras"){%>
                                                                                    <br>
                                                                                    <img src="https://media.istockphoto.com/vectors/men-working-road-sign-vector-id856987802?s=612x612"
                                                                                        alt="" width="45px"
                                                                                        height="45px"
                                                                                        class="text-center">
                                                                                    <%}%>
                                </th>
                                <th>
                                    <%= moment(clientes.Fechafirmacontrato).format('DD-MM-YYYY')%>
                                </th>
                                <th class="text-light bg-primary">
                                    <%=clientes.DocumentacionTerreno%>
                                </th>
                                <th class="text-light bg-primary">
                                    <%=clientes.DocumentacionSociedad%>

                                </th>
                                <th class="text-light bg-primary">
                                    <%=clientes.DocumentacionContractual%>
                                </th>
                                <th class="text-light bg-primary">
                                    <%=clientes.Comercial%>
                                </th>
                                <th class="text-light bg-primary">
                                    <%=clientes.Tecnica%>
                                </th>
                                 
                                <th>
                                    <%=clientes.PermisosEspeciales%>
                                </th>
                                <th>
                                    <%=clientes.DocumentacionObra%>
                                </th>
                                <th>
                                    <%=clientes.Seguridad%>
                                </th>
                                <th>
                                    <%=clientes.Interferencias%>
                                </th>
                                <th>
                                    <%=clientes.Permisos%>
                                </th>
                                <th>
                                    <%=clientes.PlanDeTrabajo%>
                                </th>
                                <th>
                                    <%=clientes.Matriculas%>
                                </th>
                                <th>
                                    <%=clientes.Ambiente%>
                                </th>
                                <th>
                                    <%=clientes.NotaCumplimentoNormativas%>
                                </th>
                                <th>
                                    <%=clientes.DDJJNAG153%>
                                   
                                </th>
                                <th>
                                    <%=clientes.Avisos%>
                                </th>
                                <th>
                                    <%=clientes.DocumentacionInspeccion%>
                                </th>
                                <th>
                                    <%=clientes.ComunicacionObras%>
                                </th>
                                <th>
                                    <%=clientes.ActasFinalesEcogas%>
                                </th>
                                <th>
                                    <%=clientes.PlanosyCroquis%>
                                </th>
                                <th>
                                    <%=clientes.ConformeEntidades %>
                                </th>
                                <th>
                                    <%=clientes.PruebaHermeticidad%>
                                </th>
                                <th>
                                    <%=clientes.InformesFinales%>
                                </th>
                                <th>
                                    <%=clientes.PresentacionFinal%>
                                </th>
                                
                                <th>
                                    <%=clientes.HabilitacionObra%>
                                </th>
                               
                                <th><a href="/edit/<%=clientes.id%>" class="btn btn-outline-info">editar</a>
                                </th>
                                 
                            </tr>
                            <% });%>    
                           
                    </tbody>
                 
                </table>
                  
            </div>
        </div>
        </div>
        </div>
        <%}%>
</body>
<script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/1.10.22/js/dataTables.bootstrap5.min.js"></script>
<!-- dataTable js -->
<!-- <script type="text/javascript" src="/main.js"></script> -->

</html>