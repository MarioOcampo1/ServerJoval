<!DOCTYPE html>
<html lang="en">

<head>
    <!-- <meta http-equiv="X-UA-Compatible" content="IE=edge"> -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Joval S.A.</title>
    <link rel="stylesheet" type="text/css"
        href="https://stackpath.bootstrapcdn.com/bootstrap/5.0.0-alpha1/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/dataTables.bootstrap5.min.css">
    <meta http-equiv="Content-Type" content="text/html;charset=UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <title>Estado General</title>
</head>

<body>
    <%- include('./partials/menusuperior.ejs')%>
    <h1 class="text-center">Estado General</h1>

        <div class="container-fluid">
            <div class="table-responsive">
                <table class="table table-striped table-bordered table-sm table-active " style="width: 100%">
                    <thead>
                        <tr>
                            <th class="text-center " colspan="6">Datos de carpeta</th>
                            <th class="text-center " colspan="5">Preliminar</th>
                            <th class="text-center " colspan="4">1era parte</th>
                            <th class="text-center " colspan="5">2da parte</th>
                            <th class="text-center " colspan="8">Caos</th>
                        </tr>
                        <tr>
                            <th>
                                <h4>Nombre</h4>
                            </th>
                            <th>
                                <h4>N° de carpeta</h4>
                            </th>
                            <th>
                                <h4>Ubicación</h4>
                            </th>
                            <th>
                                <h4>Comitente</h4>
                            </th>
                            <th>
                                <h4>Estado</h4>
                            </th>
                            <th>
                                <h4>Fecha firma de contrato</h4>
                            </th>
                        
                            <th>
                                Mensura
                            </th>
                            <th>
                                Titulo de propiedad
                            </th>
                            <th>
                                Documentación de la sociedad
                            </th>
                            <th>
                                Acta en cargo vigente
                            </th>
                            <th>
                                Cotización
                            </th>
                        
                            <th>
                                Comercial
                            </th>
                            <th>
                                PC aprobado
                            </th>
                            <th>
                                Carta oferta
                            </th>
                            <th>
                                Mail de autorización
                            </th>
                            <th>
                                Certificado de RT
                            </th>
                            <th>
                                Interferencias
                            </th>
                            <th>
                                Permisos
                            </th>
                            <th>
                                Programa de seguridad
                            </th>
                            <th>
                                Documentación de inspección
                            </th>
                            <th>
                                Planos y Croquis
                            </th>
                            <th>
                                Actas Finales
                            </th>
                            <th>
                                Informes Finales
                            </th>
                            <th>
                                Conforme de permisos
                            </th>
                            <th>
                                Presentación Final
                            </th>
                            <th>
                                Plano revisado
                            </th>
                            <th>
                                EDITAR
                            </th>


                    </thead>
                    <tbody>
                        </tr>
                        <% results.forEach((clientes)=>{ %>

                            <tr>
                                <th>
                                    <a href="/edit/<%=clientes.id%>" class=""><%=clientes.Nombre%></a>
                                </th>
                                <th>
                                    <a href="/edit/<%=clientes.id%>" class=""><%=clientes.NCarpeta%></a>
                                </th>
                                <th>
                                    <%=clientes.Ubicacion%>
                                </th>
                                <th>
                                    <%=clientes.Comitente%>
                                </th>
                                <th>
                                    <%=clientes.Estado%>
                                    <%if(clientes.Estado=="Sin presentar"){%>
                                        <progress max="4" value="0"></progress>                                  
                                          <%}%>
                                    <%if(clientes.Estado=="1ra parte"){%>
                                    <progress max="4" value="1"></progress>                           
                                         <%}%>
                                    
                                    <%if(clientes.Estado=="2da parte"){%>
                                        <progress max="4" value="2"></progress>                             
                                               <%}%>
                                        
                                    <%if(clientes.Estado=="CAOS"){%>
                                        <progress max="4" value="3"></progress>                            
                                                <%}%>
                                        
                                    <%if(clientes.Estado=="Finalizada"){%>
                                        <progress max="4" value="4"></progress>                             
                                               <%}%>
                                               <%if(clientes.Estado=="en obras"){%>
                                                <br>    
                                                <img src="https://media.istockphoto.com/vectors/men-working-road-sign-vector-id856987802?s=612x612" alt="" width="45px" height="45px" class="text-center">                            
                                                       <%}%>
                                </th>
                                <th>
                                    <%= moment(clientes.Fechafirmacontrato).format('DD-MM-YYYY')%>
                                </th>
                                <th>
                                    <%=clientes.Mensura%>
                                </th>
                                <th>
                                    <%=clientes.TituloDePropiedad%>
                                </th>
                                <th>
                                    <%=clientes.DocumentaciónSociedad%>
                                </th>
                                <th>
                                    <%=clientes.ActaCargoVigente%>
                                </th>
                                <th>
                                    <%=clientes.Cotizacion%>
                                </th>
                                <th>
                                    <%=clientes.Comercial%>
                                </th>
                                <th>
                                    <%=clientes.PCaprobado%>
                                </th>
                                <th>
                                    <%=clientes.CartaOferta%>
                                </th>
                                <th>
                                    <%=clientes.MailAutorizacion%>
                                </th>
                                <th>
                                    <%=clientes.CertificadoRT%>
                                </th>
                                <th>
                                    <%=clientes.Interferencias%>
                                </th>
                                <th>
                                    <%=clientes.Permisos%>
                                </th>
                                <th>
                                    <%=clientes.Programadeseguridad%>
                                </th>
                                <th>
                                    <%=clientes.DocInspeccion%>
                                </th>
                                <th>
                                    <%=clientes.PlanosyCroquis%>
                                </th>
                                <th>
                                    <%=clientes.ActasFinales%>
                                </th>
                                <th>
                                    <%=clientes.InformesFinales%>
                                </th>
                                <th>
                                    <%=clientes.ConformedePermisos%>
                                </th>
                                <th>
                                    <%=clientes.PresentacionFinal%>
                                </th>
                                <th>
                                    <%=clientes.PCrevisado%>
                                </th>



                                <th><a href="/edit/<%=clientes.id%>" class="btn btn-outline-info">editar</a>
                            </tr>
                            </th>
                            <% })%>
                    </tbody>
                </table>
            </div>
        </div>
        </div>
        </div>
</body>
<script type="text/javascript" language="javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" language="javascript"
    src="https://cdn.datatables.net/1.10.22/js/dataTables.bootstrap5.min.js"></script>
<!-- dataTable js -->
<!-- <script type="text/javascript" src="/main.js"></script> -->

</html>