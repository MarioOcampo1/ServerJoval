<!DOCTYPE html>
<html lang="es">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" <title
    class="text-center">
  <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Tangerine">
  <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.11.4/css/jquery.dataTables.css">

  <script type="text/javascript" charset="utf8"
    src="https://cdn.datatables.net/1.11.4/js/jquery.dataTables.js"></script>
  </title>

</head>

<body>
  <%- include('./partials/menusuperior.ejs');%>
    <% if(results){%>
      <div class="container">
        <div class="row">
          <h1 class="text-center">Historial de tareas de la carpeta </h1>
        </div>
      </div>
      <div class="container text-center">
        <div class="btn-group border-bottom ">
          <a href="/editarTareas/<%= id %>" class="btn btn-danger btn-sm">Atras </a>
        </div>
      </div>
      <br>
      <div class="table-responsive">
        <table class="table  table-bordered table-sm table-hover " id="TablaHistorial" style="width: 100%">
          <thead>
            <tr>
              <th>
                <h4>Etapa de la tarea</h4>
                <!-- <Button>Pl</Button>
                <button>1p</button> 
                <button>2p</button> 
                <button>O</button> 
                <button>C</button>  -->
              </th>
              <th>
                <h4>Fecha de tarea realizada</h4>

              </th>

              <th>
                <h4>Tarea realizada</h4>


              </th>
              <th>
                <h4>Proxima tarea</h4>

              </th>
              <th>
                <h4>Responsable</h4>
              </th>
          </thead>
          <tbody>
            </tr>
            <% results.forEach((clientes)=>{ %>
              <%if(clientes.Si_NO_TareaRealizada=='S'){%>

              <tr>
                <%if (clientes.EtapaTarea_sub=="1ra parte" ){%>
                  <th class="bg-secondary text-light">
                    <%=clientes.EtapaTarea_sub%>
                  </th>
                  <%}%>
                    <%if (clientes.EtapaTarea_sub=="Sin Presentar" ){%>
                      <th class="bg-light text-black">
                        <%=clientes.EtapaTarea_sub%>
                      </th>
                      <%}%>
                        <%if (clientes.EtapaTarea_sub=="2da parte" ){%>
                          <th class="bg-info">
                            <%=clientes.EtapaTarea_sub%>
                          </th>
                          <%}%>
                            <%if (clientes.EtapaTarea_sub==null){%>
                              <th class="bg-dark">
                                <%=clientes.EtapaTarea_sub%>
                              </th>
                              <%}%>
                                <%if (clientes.EtapaTarea_sub=="Caos" ){%>
                                  <th class="bg-warning">
                                    <%=clientes.EtapaTarea_sub%>
                                  </th>
                                  <%}%>
                                    <%if (clientes.EtapaTarea_sub=="Obras" ){%>
                                      <th class="bg-success text-light">
                                        <%=clientes.EtapaTarea_sub%>
                                      </th>
                                      <%}%>
                                        <%if (clientes.EtapaTarea_sub=="Finalizada" ){%>
                                          <th class="bg-danger text-light">
                                            <%=clientes.EtapaTarea_sub%>
                                          </th>
                                          <%}%>
                                            <th>
                                              <%=moment(clientes.Fecha_Tarea_sub).format('DD-MM-YYYY')%>
                                            </th>
                                            <th>
                                              <%=clientes.Tarea_Realizada_sub%>
                                            </th>
                                            <th>
                                              <%=clientes.Proxima_Tarea_sub%>
                                            </th>
                                            <th>
                                              <%=clientes.ResponsableDeTarea%>
                                            </th>
                                            <%}else{%>
                                              
              <tr>
                <%if (clientes.EtapaTarea_sub=="1ra parte" ){%>
                  <th class="bg-secondary text-light">
                    <%=clientes.EtapaTarea_sub%>
                  </th>
                  <%}%>
                    <%if (clientes.EtapaTarea_sub=="Sin Presentar" ){%>
                      <th class="bg-light text-black">
                        <%=clientes.EtapaTarea_sub%>
                      </th>
                      <%}%>
                        <%if (clientes.EtapaTarea_sub=="2da parte" ){%>
                          <th class="bg-info">
                            <%=clientes.EtapaTarea_sub%>
                          </th>
                          <%}%>
                            <%if (clientes.EtapaTarea_sub==null){%>
                              <th class="bg-dark">
                                <%=clientes.EtapaTarea_sub%>
                              </th>
                              <%}%>
                                <%if (clientes.EtapaTarea_sub=="Caos" ){%>
                                  <th class="bg-warning">
                                    <%=clientes.EtapaTarea_sub%>
                                  </th>
                                  <%}%>
                                    <%if (clientes.EtapaTarea_sub=="Obras" ){%>
                                      <th class="bg-success text-light">
                                        <%=clientes.EtapaTarea_sub%>
                                      </th>
                                      <%}%>
                                        <%if (clientes.EtapaTarea_sub=="Finalizada" ){%>
                                          <th class="bg-danger text-light">
                                            <%=clientes.EtapaTarea_sub%>
                                          </th>
                                          <%}%>
                                            <th class="bg-warning">
                                              <%=moment(clientes.Fecha_Tarea_sub).format('DD-MM-YYYY')%>
                                            </th>
                                            <th class="bg-warning">
                                              <%=clientes.Tarea_Realizada_sub%>
                                            </th>
                                            <th class="bg-warning">
                                              <%=clientes.Proxima_Tarea_sub%>
                                            </th>
                                            <th class="bg-warning">
                                              <%=clientes.ResponsableDeTarea%>
                                            </th>
                                              <%}%>
                                            <th>
                                              <%if(clientes.Si_NO_TareaRealizada=='S'){%>
                                                <form action=" /TareaOk/<%=clientes.Nombre_sub%>" method="post">
                                                    Si<input checked type="radio" name="TareaOK" id="TareaOK"
                                                      value="S">
                                                    <input hidden type="text" name="id" id="id"
                                                      value="<%=clientes.id%>">
                                                    <input hidden type="text" name="Nombre" id="Nombre"
                                                      value="<%=clientes.Nombre_sub%>">
                                                </form>
                                                <form action=" /TareaOk/<%=clientes.Nombre_sub%>" method="post">
                                                    No<input type="radio" onchange="this.form.submit()" name="TareaOK" id="TareaOK" value="N">
                                                  <input hidden type="text" name="id" id="id" value="<%=clientes.id%>">
                                                  <input hidden type="text" name="Nombre" id="Nombre"
                                                    value="<%=clientes.Nombre_sub%>">
                                                </form>
                                                <%}else{%>
                                                  <form action=" /TareaOk/<%=clientes.Nombre_sub%>" method="post">
                                                    <button type="submit">
                                                    Si  <input type="radio" onchange="this.form.submit()" name="TareaOK" id="TareaOK" value="S">
                                                      <input hidden type="text" name="id" id="id"
                                                        value="<%=clientes.id%>">
                                                      <input hidden type="text" name="Nombre" id="Nombre"
                                                        value="<%=clientes.Nombre_sub%>">

                                                    </button>
                                                  </form>
                                                  <form action=" /TareaOk/<%=clientes.Nombre_sub%>" method="post">

                                                    <button type="submit">
                                                      No<input checked type="radio" name="TareaOK" id="TareaOK"
                                                        value="N">
                                                    </button>
                                                    <input hidden type="text" name="id" id="id"
                                                      value="<%=clientes.id%>">
                                                    <input hidden type="text" name="Nombre" id="Nombre"
                                                      value="<%=clientes.Nombre_sub%>">


                                                  </form>
                                                  <%}%>
                                            </th>
                                            <% })%>
          </tbody>


        </table>
      </div>

</body>
<script>
  $(document).ready(function () {
    $('#comunicacionDiaria thead tr')
      .clone(true)
      .addClass('filters')
      .appendTo('#comunicacionDiaria thead');

    var url = '/historialcarpeta/:' + id;

    $('#TablaHistorial').DataTable();

  });
</script>
<%}%>

</html>