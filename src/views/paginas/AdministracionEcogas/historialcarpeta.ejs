<!DOCTYPE html>
<html lang="es">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" <title
    class="text-center">
    <link rel="stylesheet"href="https://fonts.googleapis.com/css?family=Tangerine">

  </title>

</head>

<body>
  <%- include('./partials/menusuperior.ejs');%>
    <% if(results){%>
      <div class="container">
        <div class="row">
          <h1 class="text-center">Historial de tareas de la carpeta </h1>
        </div>
      </div>
      <div class="container text-center">
        <div class="btn-group border-bottom ">

          <input type="button" value="Atras" class="btn btn-danger" onClick="history.go(-1);">
      
        </div>
      </div>
      <br>
      <div class="table-responsive">
        <table class="table table-striped table-bordered table-sm table-hover " style="width: 100%">
          <thead>
            <tr>
              <th>
                <h4>Tarea realizada</h4>
              </th>

              <th>
                <h4>Fecha de tarea realizada</h4>

              </th>
              <th>
                <h4>Etapa de la tarea</h4>

              </th>

          </thead>
          <tbody>
            </tr>
            <% results.forEach((clientes)=>{ %>

              <tr>
                <th>
                  <%=clientes.Tarea_Realizada_sub%>

                </th>

                <th>
                  <%=moment(clientes.Fecha_Tarea_sub).format('DD-MM-YYYY')%>

                </th>
                <th>
<%=clientes.EtapaTarea_sub%>
                </th>

                <% })%>
          </tbody>



        </table>
      </div>
      <%}%>
</body>

</html>