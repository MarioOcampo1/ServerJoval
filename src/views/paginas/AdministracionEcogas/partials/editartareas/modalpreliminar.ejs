<!--Preliminar-->
<th>
    <!-- Button trigger modal -->
    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#ModalPreliminar">
        Preliminar
    </button>

    <!-- Modal -->
    <div class="modal fade" id="ModalPreliminar" tabindex="-1" role="dialog" aria-labelledby="ModalPreliminar"
        aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="ModalPreliminar">
                        Preliminar</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <form action="/actualizarEtapas/<%= user.id%>" id="formPreliminar" method="post">
                        <div class="container">
                            <div class="card p-3 mt-3 border-secondary">
                                <div class="row">
                                    <div class="card-header">
                                        <label class="mx-auto" for="DocumentacionDelTerreno">Documentación
                                            del
                                            terreno</label>
                                    </div>
                                    <div class="col-6">
                                        <div class="card-title">
                                            <label for="Mensura">Mensura</label>
                                        </div>
                                        <div class="card-body">
                                            <select class="form-control" id="Mensura" name="Mensura"
                                                value="<%= tareasporcarpeta.Mensura%>"
                                                placeholder="<% tareasporcarpeta.Mensura%>">
                                                <option hidden value="<%=tareasporcarpeta.Mensura%>" selected>
                                                    <%=tareasporcarpeta.Mensura%>
                                                </option>
                                                <option value="Sin presentar">
                                                    Sin presentar
                                                </option>
                                                <option value="EnGestion">En
                                                    gestión
                                                </option>
                                                <option value="Presentado">
                                                    Presentado
                                                </option>
                                                <option value="Observado">
                                                    Observado</option>
                                                <option value="ok">ok
                                                </option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="card-title">
                                            <label for="TituloDePropiedad">Titulo
                                                de
                                                propiedad</label>
                                        </div>
                                        <div class="card-text">
                                            <select class="form-control" id="TituloDePropiedad" name="TituloDePropiedad"
                                                value="<%= tareasporcarpeta.TituloDePropiedad%>"
                                                placeholder="<%= tareasporcarpeta.TituloDePropiedad%>">
                                                <option hidden value="<%=tareasporcarpeta.TituloDePropiedad%>" selected>
                                                    <%=tareasporcarpeta.TituloDePropiedad%>
                                                </option>
                                                <option value="Sin presentar">
                                                    Sin presentar
                                                </option>
                                                <option value="EnGestion">En
                                                    gestion
                                                </option>
                                                <option value="Presentado">
                                                    Presentado
                                                </option>
                                                <option value="ok">ok
                                                </option>
                                                <option value="Observado">Observado
                                                </option>

                                            </select>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="card p-3 mt-3 border-secondary">
                                <div class="card-header">
                                    <label for="DocumentacionSociedad" class="mx-auto">
                                        Documentación de la Sociedad</label>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-4">
                                            <div class="card-title">
                                                <label for="ActaConstitutiva">
                                                    Acta
                                                    constitutiva</label>
                                            </div>
                                            <div class="card-text">
                                                <select class="form-control" id="ActaConstitutiva"
                                                    name="ActaConstitutiva"
                                                    value="<%= tareasporcarpeta.ActaConstitutiva%>"
                                                    placeholder="<%= tareasporcarpeta.ActaConstitutiva%>">
                                                    <option hidden value="<%=tareasporcarpeta.ActaConstitutiva%>"
                                                        selected>
                                                        <%=tareasporcarpeta.ActaConstitutiva%>
                                                    </option>
                                                    <option value="Sin presentar">
                                                        Sin
                                                        presentar
                                                    </option>
                                                    <option value="EnGestion">
                                                        En gestion
                                                    </option>
                                                    <option value="ok">
                                                        ok</option>
                                                    <option value="NC">
                                                        No corresponde
                                                    </option>

                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="card-title">
                                                <label for="ActaCargoVigente">
                                                    Acta en cargo
                                                    vigente</label>
                                            </div>
                                            <div class="card-text">
                                                <select class="form-control" id="ActaCargoVigente"
                                                    name="ActaCargoVigente"
                                                    value="<%= tareasporcarpeta.ActaCargoVigente%>"
                                                    placeholder="<%= tareasporcarpeta.ActaCargoVigente%>">
                                                    <option hidden value="<%=tareasporcarpeta.ActaCargoVigente%>"
                                                        selected>
                                                        <%=tareasporcarpeta.ActaCargoVigente%>
                                                    </option>
                                                    <option value="Sin presentar">
                                                        Sin
                                                        presentar
                                                    </option>
                                                    <option value="EnGestion">
                                                        En gestion
                                                    </option>
                                                    <option value="ok">
                                                        ok</option>
                                                    <option value="NC">
                                                        No corresponde
                                                    </option>

                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-4">
                                            <div class="card-title">
                                                <label for="DniComitente">
                                                    DNI del
                                                    Comitente: <%= tareasporcarpeta.DniComitente%></label>
                                            </div>
                                            <div class="card-text">
                                                <input type="text" name="DniComitente" id="DniComitente" maxlength="10"
                                                    placeholder="<%= tareasporcarpeta.DniComitente%>">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card p-3 mt-3 border-secondary">
                                <div class="card-header">
                                    Documentación Contractual
                                </div>
                                <div class="row">
                                    <div class="col-6">
                                        <div class="card-title">
                                            <label for="Cotizacion">
                                                Cotización</label>
                                        </div>
                                        <div class="card-body">
                                            <select class="form-control" id="Cotizacion" name="Cotizacion"
                                                value="<%= tareasporcarpeta.Cotizacion%>"
                                                placeholder="<%= tareasporcarpeta.Cotizacion%>">
                                                <option value="<%=tareasporcarpeta.Cotizacion%>" selected>
                                                    <%=tareasporcarpeta.Cotizacion%>
                                                </option>
                                                <option value="Sin presentar">
                                                    Sin
                                                    presentar/Hacer</option>
                                                <option value="EnGestion">En
                                                    gestion</option>
                                                <option value="ok">ok
                                                </option>

                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-6">
                                        <div class="card-title">
                                            <label for="Contrato"> Contrato
                                                preliminar</label>
                                        </div>
                                        <div class="card-body">
                                            <select class="form-control" id="ContratoPreliminar"
                                                name="ContratoPreliminar" value="<%= tareasporcarpeta.Contrato%>"
                                                placeholder="<%= tareasporcarpeta.Contrato%>">
                                                <option value="<%=tareasporcarpeta.Contrato%>" selected>
                                                    <%=tareasporcarpeta.Contrato%>
                                                </option>
                                                <option value="Sin presentar(Preliminar)">
                                                    Sin
                                                    presentar/Hacer</option>
                                                <option value="EnGestion(Preliminar)">
                                                    En gestion
                                                </option>
                                                <option value="Ok(Preliminar)">
                                                    ok</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal"
                                onclick="CerrarModal()">Cerrar</button>
                            <button type="submit" form="actPrelCarpEcogas" class="btn btn-primary fixed-bottom">Guardar
                                Cambios</button>
                        </div>
                </div>
            </div>
        </div>
</th>